// npx prisma migrate dev

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model user {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}


